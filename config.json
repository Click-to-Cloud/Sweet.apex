{
    "apiVersion": "42.0",
    "isDebugEnabled": false,
    "isPerfEnabled": false,
    "generateDoc": "off",
    "srcDir": "resources/",
    "destDir": "build/",
    "docDir": "docs/",
    "fileSrcDir": "files/",
    "fileDestDir": "build/staticresources/",
    "templateDir": "template/",
    "silent": false,
    "clean": false,
    "ignoreErrors": false,
    "scanExcludePatterns": [".*Test.cls$"],
    "features": [
        "apexdoc",
        "array_creation",
        "mod",
        "operator",
        "template_string",
        "cast",
        "lambda",
        "switch",
        "file",
        "inject",
        "not_null",
        "default_value",
        "optional",
        "rethrow",
        "log",
        "function",
        "action",
        "identity",
        "reflect",
        "enum"
    ]
}
